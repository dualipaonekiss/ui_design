        -in $site_3_0 -x 0 -relx 0.013 -y 0 -rely 0.73 -width 0 \
        -relwidth 0.17 -height 0 -relheight 0.061 -anchor nw \
        -bordermode ignore 
    place $site_3_0.fra97 \
        -in $site_3_0 -x 0 -relx 0.252 -y 0 -rely 0.73 -width 0 \
        -relwidth 0.736 -height 0 -relheight 0.066 -anchor nw \
        -bordermode ignore 
    place $site_3_0.lab98 \
        -in $site_3_0 -x 0 -relx 0.013 -y 0 -rely 0.832 -width 0 \
        -relwidth 0.17 -height 0 -relheight 0.06 -anchor nw \
        -bordermode ignore 
    place $site_3_0.fra99 \
        -in $site_3_0 -x 0 -relx 0.252 -y 0 -rely 0.832 -width 0 \
        -relwidth 0.736 -height 0 -relheight 0.066 -anchor nw \
        -bordermode ignore 
    place $site_3_0.lab100 \
        -in $site_3_0 -x 0 -relx 0.05 -y 0 -rely 0.922 -width 0 \
        -relwidth 0.167 -height 0 -relheight 0.062 -anchor nw \
        -bordermode ignore 
    place $site_3_0.lab101 \
        -in $site_3_0 -x 0 -relx 0.39 -y 0 -rely 0.922 -width 0 \
        -relwidth 0.167 -height 0 -relheight 0.059 -anchor nw \
        -bordermode ignore 
    place $site_3_0.lab102 \
        -in $site_3_0 -x 0 -relx 0.767 -y 0 -rely 0.922 -width 0 \
        -relwidth 0.181 -height 0 -relheight 0.059 -anchor nw \
        -bordermode ignore 
    ###################
    # SETTING GEOMETRY
    ###################
    place $top.lab47 \
        -in $top -x 0 -relx 0.169 -y 0 -rely 0.013 -width 0 -relwidth 0.674 \
        -height 0 -relheight 0.079 -anchor nw -bordermode ignore 
    place $top.fra48 \
        -in $top -x 0 -relx 0.007 -y 0 -rely 0.113 -width 0 -relwidth 0.465 \
        -height 0 -relheight 0.878 -anchor nw -bordermode ignore 
    place $top.fra49 \
        -in $top -x 0 -relx 0.475 -y 0 -rely 0.113 -width 0 -relwidth 0.518 \
        -height 0 -relheight 0.876 -anchor nw -bordermode ignore 

    vTcl:FireEvent $base <<Ready>>
}

proc 36 {args} {return 1}


Window show .
set btop1 ""
if {$vTcl(borrow)} {
    set btop1 .bor[expr int([expr rand() * 100])]
    while {[lsearch $btop1 $vTcl(tops)] != -1} {
        set btop1 .bor[expr int([expr rand() * 100])]
    }
}
set vTcl(btop) $btop1
Window show .top1 $btop1
if {$vTcl(borrow)} {
    $btop1 configure -background plum
}

